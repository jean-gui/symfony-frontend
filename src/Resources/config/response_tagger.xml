<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
        https://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>

        <service id="strata.event_subscriber.response_helper" class="Strata\SymfonyBundle\EventSubscriber\ResponseHelperEventSubscriber">
            <tag name="kernel.event_subscriber" />
            <argument id="strata.response_helper" type="service" />
            <argument id="fos_http_cache.response_tagger" type="service" />
            <argument id="strata.query_manager" type="service" />
        </service>

    </services>

</container>
